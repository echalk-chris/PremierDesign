	font-size: 14px;
	font-weight: bold;
	text-decoration: none;
}

#archive-list a:hover {
	color: $hilight;
	text-decoration: underline;
}

#archive-list .date {
	font-size: 12px;
	font-weight: bold;
	color: $medium-dark;
	padding-left: 4px;
}	

.viewlink {
	font-family: 'Cabin', sans-serif;
	padding: 0px;
	margin: 3px 0 0 0;
	float: right;
	font-size: 11px;
	line-height: 20px;
	width: 70px;
	background: url(../img/aechs_viewall.jpg) no-repeat;
	height: 20px;
	font-weight: bold;
	text-decoration: none;
	transition: all ease-in-out 0.2s;
	-moz-transition: all ease-in-out 0.2s;
	-webkit-transition: all ease-in-out 0.2s;
	-o-transition: all ease-in-out 0.2s;
	color: $dark;
	text-transform: uppercase;
	text-align: center;
	
}

.viewlink:hover {
	color: $hilight;
	text-decoration: underline;
}

#upcoming-events {
	margin-bottom: 32px;
}

#news-archive {
	margin-bottom: 18px;
}

#daily-announcements {
	overflow: hidden;
	margin-bottom: 18px;
}

#announcement-list {
	height: 159px;
	overflow: auto;
	padding: 0px 7px;
	clear: both;
}

#announcement-list .media {
	padding: 0px;
	border-top: 1px solid $medium;
	margin: 0px;
}

a.pull-left {
	float: left;
}

a.pull-left img {
	border: 1px solid $secondary;
}

#announcement-list .media:first-child { border-top: 0px; }

#announcement-list .media-heading {
	font-size: 16px;
}

#announcement-list .media-heading a:hover {
	color: $medium-dark;
	text-decoration: none;
}

.annbody {
	font-size: 13px;
	line-height: 17px;
}

.annbody hr {
	background: $dark;
	height: 1px;
	border: 0px;
	margin: 10px 0;
	padding: 0;
}

.annbody h2 {
	font-size: 16px;
	line-height: 18px;
	margin: 5px 0 0 0;
	padding: 0px;
	letter-spacing: 0px;
}

h3.anndate {
	font-size: 12px;
	font-weight: normal;
	margin: 0 0 8px 0;
	padding: 0;
	color: $secondary;
	letter-spacing: 1px;
}

#announcement-list .media p {
	text-align: left !important;
	margin: 5px 0;
}

#announcement-list .media p span {
	font-size: 13px !important;
	line-height: 17px !important;
	font-variant: normal !important;
}

.urgenticon {
	background: url(../img/urgenticon.png) no-repeat;
	width: 16px;
	height: 16px;
	display: inline-block;
	margin-right: 3px;
}

.attach-icon {
	background: url(../img/attach_alief_gray.png) no-repeat;
	width: 12px;
	height: 12px;
	display: inline-block;
	margin-left: 5px;
}

.more-btn {
	background: $secondary;
	color: $light;
	text-transform: uppercase;
	font-size: 12px;
	line-height: 12px !important;
	display: inline-block;
	padding: 5px;
	text-decoration: none;
	transition: all ease-in-out 0.2s;
	-moz-transition: all ease-in-out 0.2s;
	-webkit-transition: all ease-in-out 0.2s;
	-o-transition: all ease-in-out 0.2s;
	margin-left: 7px;
}

.more-btn:hover {
	background: $primary;
	color: $light;
	text-decoration: none;
}

.wbox .header h2 {
	color: $secondary;
	font-size: 17px;
	line-height: 28px;
	letter-spacing: 0px;
	margin: 0px;
	padding: 0px 2px 2px 2px;
	float: left;
}

.wbox .header h2 a, .wbox .header h2 a:hover, .wbox .header h2 a:active {
	color: $hilight;
	text-decoration: none;
}

.homebox header h2 .icon {
	display: inline-block;
	height: 26px;
	margin: 0px 1px -7px 0px;
	background: url(../img/aechs_sprite.png) no-repeat;
}

.homebox header h2 .icon.speaker { background-position: 0px -172px; width: 26px; }
.homebox header h2 .icon.note { background-position: -27px -172px; width: 26px; }
.homebox header h2 .icon.person { background-position: -54px -172px; width: 22px; }
.homebox header h2 .icon.calendar { background-position: -77px -172px; width: 26px; }

.home-rss {
	display: inline-block;
	width: 16px;
	height: 15px;
	background: url(../img/rss.png) no-repeat;
	margin-bottom: -2px;
}

.home-rss a {
	display: block;
	width: 100%;
	height: 100%;
}

#mission-list {
	height: 120px;
	overflow: auto;
	padding: 7px 7px 0px 7px;
}

#message-list {
	height: 340px;
	overflow: auto;
	padding: 0px 7px;
}



.transition {
	transition: all ease-in-out 0.2s;
	-moz-transition: all ease-in-out 0.2s;
	-webkit-transition: all ease-in-out 0.2s;
	-o-transition: all ease-in-out 0.2s;
}

.reveal-modal {
	overflow: auto;
	max-height: 700px;
}

.reveal-modal h2 {
	font-size: 24px;
}

.reveal-modal h3 {
	color: $secondary;
}

.reveal-modal a {
	color: $secondary;
	text-decoration: underline;
}

.reveal-modal img {
	max-width: 100%;
}

.reveal-modal a:hover {
	color: $medium-dark;
}

.reveal-modal .close-reveal-modal {
	text-decoration: none;
}

.news-title {
	font-size: 20px !important;
	font-weight: normal;
	border-bottom: 2px solid $medium;
	padding-bottom: 6px;
	margin-bottom: 18px;
}

#mobile-motto {
	display: none;
}

.caps {
	text-transform: uppercase;
}

/* ==========================================================================
   eChalk Styles!
   ========================================================================== */

.topNavOnly {
	width: 100%;
}

.wrapper {
	width: 100%;
	max-width: 950px;
	overflow-x: auto;
}

.mainContainer {
	width: 100%;
	max-width: 100%;
	overflow-x: auto;
	background: $light;
	position: relative;
	z-index: 2;
}

.utilityNav {
	display: none;
}

.header.template {
	border-radius: 0px;
	display: none;
}

.navRpt {
	display: none;
}

.breadCrumbs {
	margin: 0px 10px 10px 10px;
	font-size: 13px;
}

.rootBreadCrumb, .parentBreadCrumb { /* breadcrumb text */
	color: #d95233;
}

.currentBreadCrumb, .rootBreadCrumb:hover, .parentBreadCrumb:hover { /* breadcrumb text */
	color: $secondary;
}

span.breadCrumbSeperator { /* separator between breadcrumbs */
	background: transparent;
	margin: 0px 3px;
	text-indent: -3px;
}

span.breadCrumbSeperator:after {
	content:"\00bb";
	color: $primary;
}

.content {
	margin: 20px 0px;
}

.btmCurve {
	display: none;
}

h1.pageTitleContainer {
	font-family: Verdana, serif;
}

h1, h2, h3, h4, h5 {
	font-family: Verdana, serif;
}

#ctl00_body .footer {
	width: 100%;
	padding: 0px;
	background: transparent;
}

#ctl00_body .footer * {
	color: inherit;
}

.backLink { /* bottom back to link area */
	margin-bottom: 10px;
}

.backLink a { /* bottom back to link */
	padding: 5px 10px;
	background: $secondary;
	color: $light;
	text-decoration: none;
	transition: all ease-in-out 0.2s;
	font-weight: bold;
}

.backLink a:hover {
	background: $primary;
	text-decoration: none;
}

	
/* ==========================================================================
   Media Queries
   ========================================================================== */

@media only screen and (min-width: 941px) {

	ul.title-area {
		display: none;
	}

	.top-bar.expanded .title-area {
		background: $light !important;
	}
	
	.top-bar-section .dropdown label:first-child {
		display: none;
	}
	
}
   
@media only screen and (min-width: 58.75em) {
	
	.top-bar-section .has-dropdown > a {
		padding-right: 26px !important;
	}
	
	.top-bar-section .dropdown li label {
		background: $light;
	}
	
	.top-bar-section .has-dropdown > a:after {
		margin-top: -3px;
	}
	
	.has-dropdown.edge > ul {
		left: auto;
		right: 0;
		text-align: right;
	}
	
	.pos_right{
	  float: right;
	  left: auto !important;
	  right: 0;
	}

	.pos_left{
	  float: left;
	  left: 0;
	  right: auto !important;
	}
	
	.top-bar-section .dropdown {
		-webkit-box-shadow: 3px 3px 6px rgba(50, 50, 50, 0.6);
		-moz-box-shadow:    3px 3px 6px rgba(50, 50, 50, 0.6);
		box-shadow:         3px 3px 6px rgba(50, 50, 50, 0.6);
	}
	
}
   
@media only screen and (max-width: 940px) {

	.top-bar .name {
		display: block;
	}
	
	.top-bar-section li a:not(.button) {
		padding: 7px;
		line-height: 22px;
	}

}

@media only screen and (min-width: 768px) and (max-width: 783px) {
	#home-quicklinks li {
		text-align: center;
	}
	
	#home-quicklinks li a span {
		display: block;
		margin: 0px auto 3px auto;
	}	
}

@media only screen and (max-width: 767px) {

	#login-area {
		margin-top: 10px;
	}

	#ctl00_body #content-container {
		width: 96%;
	}

	header.header {
		margin-top: 6px;
	}

	#header-top, .nav-under {
		display: none;
	}

	#content-box-subpages {
		border: 7px solid $light;
		padding: 10px;
		width: 100%;
	}

	
	#mobile-links {
		display: block;
	}
	
	#logo {
		width: 180px;
		height: 102px;
		background-size: 180px auto;
		margin: 20px 0 0 4px;
	}
	
	#content-box {
		width: 100%;
		padding: 10px;
	}

	#home-slideshow {
		width: 100%;
		float: none;
		max-width: 100%;
		margin-right: 0px;
	}
	
	#home-right {
		position: relative;
		width: 100%;
		float: none;
		left: 0px;
		top: 0px;
		box-shadow: none;
		height: 100% !important;
		max-height: 100%;
		overflow: auto;
	}
	
	#home-announcements {
		padding: 10px;
	}
	
	#the-date {
		position: relative;
		display: block;
		width: 100%;
		text-align: center;
		margin: 36px 0 0 0;
		clear: both;
		top: 0;
		left: 0;
	}
	
	.nbs-flexisel-ul {
		padding: 20px 0 0 0;
	}
	
	.nbs-flexisel-nav-left, .nbs-flexisel-nav-right {
		top: -59px !important;
	}
	
	.nbs-flexisel-nav-left {
		left: -10px;
	}
	
	.nbs-flexisel-nav-right {
		left: 21px;
	}

	#col-1, #col-2, #col-3 {
		float: none;
		width: 100%;
	}
	
	#google_translate_element {
		float: none;
	}

	#header-top h1 {
		font-size: 19px;
		margin-top: 24px;
		line-height: 24px;
	}
	
	#header-top h1 #motto {
		font-size: 15px;
	}
	
	#quick-links {
		margin-top: 12px;
	}
	
	#events-list, #message-list, #mission-list, #announcement-list {
		height: 100%;
		display: block;
	}
	
	#upcoming-events {
		margin-bottom: 18px;
	}
	
	#echalk-logo {
		display: block;
		margin: 0 0 10px 100%;
		clear: both;
	}
	
	#left-col, #right-col {
		float: none;
		display: block;
		width: 100%;
	}
	
	#left-col {
		margin-bottom: 20px;
	}
	
	.slider-nav.hide-for-small {
		display: block !important;
	}

}

@media only screen and (min-width: 768px) {

	.sf-menu {
		display: block !important;
	}

}

@media print,
       (-o-min-device-pixel-ratio: 5/4),
       (-webkit-min-device-pixel-ratio: 1.25),
       (min-resolution: 120dpi) {

}

/* ==========================================================================
   Helper classes
   ========================================================================== */

.ir {
    background-color: transparent;
    border: 0;
    overflow: hidden;
    *text-indent: -9999px;
}

.ir:before {
    content: "";
    display: block;
    width: 0;
    height: 150%;
}

.hidden {
    display: none !important;
    visibility: hidden;
}

.visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus {
    clip: auto;
    height: auto;
    margin: 0;
    overflow: visible;
    position: static;
    width: auto;
}

.invisible {
    visibility: hidden;
}

.clearfix:before,
.clearfix:after {
    content: " ";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}

/* ==========================================================================
   Print styles
   ========================================================================== */

@media print {
    * {
        background: transparent !important;
        color: $dark !important; /* Black prints faster: h5bp.com/s */
        box-shadow: none !important;
        text-shadow: none !important;
    }

    a,
    a:visited {
        text-decoration: underline;
    }

    a[href]:after {
        content: " (" attr(href) ")";
    }

    abbr[title]:after {
        content: " (" attr(title) ")";
    }

    /*
     * Don't show links for images, or javascript/internal links
     */

    .ir a:after,
    a[href^="javascript:"]:after,
    a[href^="#"]:after {
        content: "";
    }

    pre,
    blockquote {
        border: 1px solid #999;
        page-break-inside: avoid;
    }

    thead {
        display: table-header-group; /* h5bp.com/t */
    }

    tr,
    img {
        page-break-inside: avoid;
    }

    img {
        max-width: 100% !important;
    }

    @page {
        margin: 0.5cm;
    }

    p,
    h2,
    h3 {
        orphans: 3;
        widows: 3;
    }

    h2,
    h3 {
        page-break-after: avoid;
    }
