}

.carousel-inner > .item > img, .carousel-inner > .item > a > img {
	max-height: 460px;
	min-width: 100%;
}

.orbit-caption {
	font-size: 12px !important;
	text-align: left;
	padding: 7px !important;
	background: #3a3a3a;
	background: rgba(0,0,0,0.6);
	color: $light;
	width: 100%;
	position: absolute;
	bottom: -1px;
	left: 0px;
}

.orbit-caption a {
	color: $secondary !important;
	text-decoration: none;
}

.orbit-caption a:hover {
	color: $secondary !important;
	text-decoration: underline;
}

#home-slideshow .carousel-caption a {
	color: #009968;
	text-decoration: none;
	font-size: 12px;
	font-family: 'Cabin', sans-serif;
	text-transform: uppercase;
	padding-left: 3px;
}

#home-slideshow .carousel-caption a:hover {
	color: $medium;
	text-decoration: none;
}


#home-slideshow .carousel-control, .orbit-wrapper .slider-nav span, .slide-prev, .slide-next {
	width: 38px;
	height: 38px !important;
	opacity: 1;
	filter: alpha(opacity=100);
	-webkit-border-radius: 0px;
	-moz-border-radius: 0px;
	border-radius: 0px !important;
	background: url(../img/main_sprite.png) no-repeat !important;
	border: 0px;
	cursor: pointer;
	top: 50%;
	margin-top: -19px !important;
	text-indent: -9999px;
	display: block;
	position: absolute;
}

.orbit-prev, .orbit-next {
	display: none !important;
}

.orbit-prev span, .orbit-next span {
	display: none !important;
}

.orbit-timer, .orbit-slide-number {
	visibility: hidden;
}

#home-slideshow .carousel-control:hover {
	opacity: 1;
	filter: alpha(opacity=100);
}
	

#home-slideshow .carousel-control.left, .slide-prev {
	background-position: 0px 0px !important;
	left: 0px;
}

#home-slideshow .carousel-control.right, .slide-next {
	background-position: -39px 0px !important;
	right: 0px;
}

.orbit-prev:hover, .slide-prev:hover { background-position: 0px -39px !important; }
.orbit-next:hover, .slide-next:hover { background-position: -39px -39px !important; }

